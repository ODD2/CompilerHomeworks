(*
 * Example with Functions
 *)

module a07

// constants
const x = 90;
// variables
var c: integer;
    
procedure div(a:integer, b:integer) : integer
begin
  return a / b;
end div;
    
// main function
begin
  c := div(x, 9);
  if (c = 10) then
    println -c;
  else 
    println c;
  end;
end a07.
